    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building the master branch..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the master branch..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the master branch....'
            }
        }
    }
}
